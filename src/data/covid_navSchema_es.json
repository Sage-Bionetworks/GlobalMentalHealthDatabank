{
  "steps": [
    {
      "id": "symptoms",
      "order": 1,
      "title": "Experiencia reciente con COVID-19",
      "orderDisplay": "1/7",
      "description": "Por favor complete las preguntas de este cuestionario, a lo que mejor pueda. Si usted no se recuerda o no desea responder a la pregunta, por favor marque \"Prefiero no responder\" ",

      "default": "symptoms1",
      "validationRules": [
        {
          "conditions": {
            "all": [
              {
                "fact": "symptoms",
                "operator": "symptomsDates",

                "value": {
                  "fact": "symptoms",
                  "path": ".symptoms_start.symptoms_start_date"
                }
              }
            ]
          },
          "event": {
            "type": "validation",

            "params": {
              "message": "please check that your symptoms dates are in the right sequence",
              "name": "range",
              "property": ".symptoms"
            }
          },
          "priority:": 1
        }
      ]
    },
    {
      "id": "symptoms1",
      "order": 5,
      "title": "",
      "orderDisplay": "2/7",
      "validationRules": [
        {
          "conditions": {
            "all": [
              {
                "fact": "symptoms1",
                "operator": "notHasChecked",
                "path": ".symptoms_list",
                "value": true
              }
            ]
          },
          "event": {
            "type": "validation",
            "params": {
              "message": "   ",
              "name": "required",
              "property": "symptoms1.symptoms_list"
            }
          },
          "priority:": 1
        }
      ],
      "default": "symptoms2"
    },
    {
      "id": "symptoms2",
      "orderDisplay": "3/7",
      "order": 10,
      "title": "",
      "default": "medications",
      "rules": [
        {
          "conditions": {
            "all": [
              {
                "fact": "symptoms2",
                "operator": "in",
                "path": ".how_was_it_treated",
                "value": ["icu", "hospital", "emergency_room"]
              }
            ]
          },
          "event": {
            "type": "next",
            "params": {
              "next": "hospitalization"
            }
          },
          "priority:": 1
        }
      ],
      "validationRules": [
        {
          "conditions": {
            "all": [
              {
                "fact": "symptoms2",
                "operator": "notHasChecked",
                "path": ".kind_of_testing",
                "value": true
              }
            ]
          },
          "event": {
            "type": "validation",
            "params": {
              "message": "   ",
              "name": "required",
              "property": "symptoms2.kind_of_testing"
            }
          },
          "priority:": 1
        }
      ]
    },

    {
      "id": "hospitalization",
      "orderDisplay": "3a/7",
      "order": 15,
      "title": "",
      "default": "medications"
    },

    {
      "id": "medications",
      "orderDisplay": "4/7",
      "order": 30,
      "title": "",
      "default": "medications2"
    },

    {
      "id": "medications2",
      "orderDisplay": "5/7",
      "order": 35,
      "title": "",
      "default": "habits",
      "rules": [
        {
          "conditions": {
            "all": [
              {
                "fact": "medications2",
                "operator": "contains",
                "path": ".covid_medications",
                "value": "Hydroxychloroquine (Plaquenil) or Chloroquine"
              }
            ]
          },
          "event": {
            "type": "next",
            "params": {
              "next": "medications_hydroxy"
            }
          },
          "priority:": 1
        }
      ]
    },
    {
      "id": "medications_hydroxy",
      "orderDisplay": "5a/7",
      "order": 40,
      "title": "",
      "default": "habits"
    },

    {
      "orderDisplay": "6/7",
      "id": "habits",
      "order": 45,
      "title": "",
      "default": "habits2"
    },
    {
      "id": "habits2",
      "orderDisplay": "7/7",
      "order": 50,
      "title": "",
      "default": ""
    }
  ]
}
