{
  "attributes": {
    "classNames": "demographic",
    "address": {
      "classNames": "address"
    },
    "zip_code": {
      "classNames": "zip"
    },
    "state": {
      "classNames": "state"
    },

    "gender": {
      "ui:widget": "radio",
      "ui:options": {
        "inline": false
      }
    },

    "dob": {
      "classNames": "birthdate",
      "ui:widget": "alt-date",
      "ui:options": {
        "yearsRange": [1910, 2001],
        "hideNowButton": true,
        "hideClearButton": true
      }
    }
  }
}
